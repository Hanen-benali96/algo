ALGORITHM algoprocedure
VAR
    v1,v2 : ARRAY_OF INTEGER[50];
    sum:INTEGER:=0;
BEGIN

   proVecProduit(vec1,vec2,sum)
    IF (sum=0) THEN
                  write ("vecteurs orth")

    ELSE
                 write("vecteurs non orth")

    END_IF
END



PROCEDURE proVecProduit(VAR sum ,v1,v2:ARRAY_OF INTEGER)
    VAR
        i:INTEGER
    BEGIN
                 FOR i FROM 0 TO v1.length - 1  DO
                      sum=sum+(v1[i]*v2[i])
                 END_FOR